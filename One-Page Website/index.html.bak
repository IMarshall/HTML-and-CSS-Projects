		<!-- ORDER FORM -->
		<section>
			<div id="contact">
				<div>
					<h1>Order Form</h1>
					<div>
						<h3><span>Please fill out the order form below.</span>
							<br />We will email you promptly with a quote.
						</h3>
						<p>
							Please provide us with your basic contact information and the custom features for your fidget spinner.
						</p>
						<div>
							<form action="" method="post">
								<input class="rsvp" type="text" value="First Name">
								<input class="rsvp" type="text" value="Last Name">
								<input class="rsvp" type="email" value="Email Address">
								<input class="rsvp" type="text" value="Phone Number">
								<label for="materials">Choose a material:</label>
								<select id="materials" name="materials">
									<option value="plastic">Plastic</option>
									<option value="aluminum">Aluminum</option>
									<option value="copper">Copper</option>
									<option value="titanium">Titanium</option>
								</select>
								<label for="bearings">Choose a bearing:</label>
								<select id="bearings" name="bearings">
									<option value="steel">Steel</option>
									<option value="ceramic">Ceramic</option>
								</select>
								<label for="color_choice">Select your color:</label>
								<input type="color" id="color_choice" name="color_choice">
								<div class="form-spacer">
									<label class="terms">Terms and Conditions</label>
								</div>
								<div>
									<input type="submit" value="Submit">
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
